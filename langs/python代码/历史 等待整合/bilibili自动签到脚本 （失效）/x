$(document).ready(function(){
function setwidth(){
	if(screen.width <= 1024){$('.switchbtn').click();}
	
	var selectwidth = getCookie('selectwidth');
	
	if (selectwidth) {
		jQuery('#selector').css('width', selectwidth + 'px');
	}
}
setwidth();
