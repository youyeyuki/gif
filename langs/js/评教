


JavaScript:(function(){

	iframeFirst= document.getElementById("iframeautoheight");
innerDocFirst = iframeFirst.contentDocument||iframe.contentWindow.document;
pjkc_count=innerDocFirst.getElementsByName("pjkc")[0].options.length;
getRadom =function (){
var num = Math.ceil(Math.random()*2);
if(num==0){
num+=1;
}
return num;
}
select_data= function(){
iframe= document.getElementById("iframeautoheight");
innerDoc = iframe.contentDocument||iframe.contentWindow.document;
node_count = innerDoc.getElementsByClassName("FFF").length;
for(var i=2;i<=node_count+1;i++){
	op = innerDoc.getElementById("DataGrid1"+"__ctl"+i+"_JS1");
op.options[getRadom()].selected=true;
}
innerDoc.getElementById("Button1").click();	
}
for(var kc_increase=1;kc_increase<=pjkc_count;kc_increase++){
console.log(kc_increase*3000);
setTimeout(select_data,kc_increase*3000);
}

})();






var iframeFirst= document.getElementById("iframeautoheight");
var innerDocFirst = iframeFirst.contentDocument||iframe.contentWindow.document;
var pjkc_count=innerDocFirst.getElementsByName("pjkc")[0].options.length;
var count=0;
var getRadom =function (){
var num = Math.ceil(Math.random()*2);
if(num==0){
num+=1;
}
return num;
}
var select_data= function(){
	count++;
var iframe= document.getElementById("iframeautoheight");
var innerDoc = iframe.contentDocument||iframe.contentWindow.document;
var node_count = innerDoc.getElementsByClassName("FFF").length;
for(var i=2;i<=node_count+1;i++){
	var op = innerDoc.getElementById("DataGrid1"+"__ctl"+i+"_JS1");
op.options[getRadom()].selected=true;
}
innerDoc.getElementById("Button1").click();	
	if(pjkc_count==count){
	innerDoc.getElementById("Button2").click();
	}
}
for(var kc_increase=1;kc_increase<=pjkc_count;kc_increase++){
//console.log(kc_increase*3000);
setTimeout(select_data,kc_increase*3000);
}
